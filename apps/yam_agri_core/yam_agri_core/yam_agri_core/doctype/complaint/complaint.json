{
  "doctype": "DocType",
  "name": "Complaint",
  "module": "YAM Agri Core",
  "custom": 1,
  "track_changes": 1,
  "autoname": "naming_series:",
  "title_field": "customer_name",
  "fields": [
    {
      "fieldname": "naming_series",
      "fieldtype": "Select",
      "label": "Naming Series",
      "options": "YAM-COMP-.YYYY.-",
      "default": "YAM-COMP-.YYYY.-"
    },
    {"fieldname": "site", "fieldtype": "Link", "options": "Site", "label": "Site", "reqd": 1},
    {"fieldname": "complaint_date", "fieldtype": "Date", "label": "Complaint Date"},
    {"fieldname": "customer_name", "fieldtype": "Data", "label": "Customer Name"},
    {"fieldname": "lot", "fieldtype": "Link", "options": "Lot", "label": "Lot"},
    {"fieldname": "description", "fieldtype": "Small Text", "label": "Description"},
    {
      "fieldname": "status",
      "fieldtype": "Select",
      "options": "Open\nInvestigating\nClosed\nEscalated",
      "label": "Status"
    },
    {"fieldname": "resolution", "fieldtype": "Small Text", "label": "Resolution"}
  ],
  "permissions": [
    {"role": "System Manager", "read": 1, "write": 1, "create": 1, "delete": 0},
    {"role": "QA Manager", "read": 1, "write": 1, "create": 1, "delete": 0}
  ]
}
