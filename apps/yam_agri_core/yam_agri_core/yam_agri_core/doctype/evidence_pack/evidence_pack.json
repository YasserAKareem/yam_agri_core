{
  "doctype": "DocType",
  "name": "EvidencePack",
  "module": "YAM Agri Core",
  "custom": 1,
  "fields": [
    {"fieldname": "title", "fieldtype": "Data", "label": "Title"},
    {"fieldname": "site", "fieldtype": "Link", "options": "Site", "label": "Site"},
    {"fieldname": "from_date", "fieldtype": "Date", "label": "From Date"},
    {"fieldname": "to_date", "fieldtype": "Date", "label": "To Date"},
    {
      "fieldname": "status",
      "fieldtype": "Select",
      "options": "Draft\nPrepared\nApproved\nRejected",
      "label": "Status"
    },
    {"fieldname": "notes", "fieldtype": "Small Text", "label": "Notes"}
  ],
  "permissions": [
    {"role": "System Manager", "read": 1, "write": 1, "create": 1, "delete": 0},
    {"role": "QA Manager", "read": 1, "write": 1, "create": 1, "delete": 0}
  ]
}
