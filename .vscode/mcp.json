{
  "servers": {
    "github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "gallery": true
    },
    "frappe-filesystem": {
      "type": "stdio",
      "command": "python",
      "args": [
        "tools/mcp/launch_mcp.py",
        "@modelcontextprotocol/server-filesystem",
        "${workspaceFolder}"
      ],
      "env": {}
    },
    "frappe-api": {
      "type": "stdio",
      "command": "python",
      "args": ["tools/mcp/launch_mcp.py", "fetch-mcp"],
      "env": {
        "ALLOWED_HOSTS": "localhost,dev.local,127.0.0.1"
      }
    }
  }
}
